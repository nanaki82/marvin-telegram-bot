#: Marvin/modules/events/event_command.py:64
msgid "The usage of reminder command is: /reminder <hours> <event_name>"
msgstr ""
"L’uso del comando martellamento e’: /reminder <hours> <event_name>"

#: Marvin/modules/events/event_command.py:70
msgid "The first argument must be an integer (hours)"
msgstr "Il primo argomento deve essere un intero"

#: Marvin/modules/events/event_command.py:85
msgid "You can't set a reminder on events created by others"
msgstr ""
"Non puoi impostare un martellamento con un evento creato da un altro"

#: Marvin/modules/events/event_command.py:120
msgid "Reminder removed!"
msgstr "Martellamento rimosso!"

#: Marvin/modules/events/event_command.py:139
msgid "Bye! I hope we can talk again some day"
msgstr "Ci vediamo presto"

#: Marvin/modules/events/event_command.py:150
msgid "You don't have the permission to create new event"
msgstr "Non puoi creare eventi"

#: Marvin/modules/events/event_command.py:159
msgid "(1/4) Insert the title of the event"
msgstr "(1/4) Che dobbiamo fa?"

#: Marvin/modules/events/event_command.py:172
msgid "(2/4) Ok! Now set a description or /skip"
msgstr "(2/4) Che roba e’? or /skip"

#: Marvin/modules/events/event_command.py:184
#: Marvin/modules/events/event_command.py:190
msgid ""
"(3/4) Ok! Now set the date and the time (dd/mm/yyyy HH:mm format, eg "
"30/10/1970 15:33)"
msgstr "(3/4) Quando si fa? esempio -> 30/10/1970 15:33"

#: Marvin/modules/events/event_command.py:198
msgid "The date is in the past, insert correct date!"
msgstr "Hai sbagliato data!"

#: Marvin/modules/events/event_command.py:207
msgid "(4/4) Last step! Set the location or /skip"
msgstr "(4/4) Addo’ si fa? or /skip"

#: Marvin/modules/events/event_command.py:219
#: Marvin/modules/events/event_command.py:229
msgid "Yeah!!! Event created"
msgstr "Iammi nu poc!!"

#: Marvin/modules/events/event_command.py:236
msgid "There is no draft open for user {}"
msgstr "Non ci sono bozze aprte per l’utente {}"

#: Marvin/modules/events/event_inline.py:123
msgid "Users confirmed (<b>{total}</b>)\n"
msgstr "Non mollano (<b>{total}</b>)\n"

#: Marvin/modules/events/event_inline.py:133
msgid "Users not confirmed ({total})\n"
msgstr "Stanno mollando ({total})\n"

#: Marvin/modules/events/event_inline.py:143
msgid "Users maybe ({total})\n"
msgstr "Forse mollano ({total})\n"

#: Marvin/modules/events/event_inline.py:156
msgid "Join"
msgstr "Non mollo"

#: Marvin/modules/events/event_inline.py:160
msgid "Not go"
msgstr "Mollo"

#: Marvin/modules/events/event_inline.py:164
msgid "Maybe"
msgstr "Forse mollo"

#: Marvin/modules/events/event_model.py:47
msgid "No user_id found in dictionary"
msgstr "Nessun utente trovato (user_id)"
