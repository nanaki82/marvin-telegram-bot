#: Marvin/modules/events/event_command.py:64
msgid "The usage of reminder command is: /reminder <hours> <event_name>"
msgstr "L'utilizzo del comando è: /reminder <hours> <event_name>"

#: Marvin/modules/events/event_command.py:70
msgid "The first argument must be an integer (hours)"
msgstr "Il primo argomento deve essere un valore intero (ore)"

#: Marvin/modules/events/event_command.py:85
msgid "You can't set a reminder on events created by others"
msgstr ""
"Non è possibile impostare un promemoria su eventi creati da altri utenti"

#: Marvin/modules/events/event_command.py:113
msgid "There are no active timer"
msgstr "Non ci sono timer attivi"

#: Marvin/modules/events/event_command.py:121
msgid "You can't remove the timer"
msgstr "Non puoi rimuovere il timer"

#: Marvin/modules/events/event_command.py:127
msgid "Reminder removed!"
msgstr "Rimosso il promemoria!"

#: Marvin/modules/events/event_command.py:146
msgid "Bye! I hope we can talk again some day"
msgstr "Ciao! Spero che un giorno tornerai a parlare con me"

#: Marvin/modules/events/event_command.py:157
msgid "You don't have the permission to create new event"
msgstr "Non hai i permessi per creare un nuovo evento"

#: Marvin/modules/events/event_command.py:166
msgid "(1/4) Insert the title of the event"
msgstr "(1/4) Inserisci il titolo dell’evento"

#: Marvin/modules/events/event_command.py:179
msgid "(2/4) Ok! Now set a description or /skip"
msgstr "(2/4) Ok! Ora imposta la descrizione oppure /skip"

#: Marvin/modules/events/event_command.py:191
#: Marvin/modules/events/event_command.py:197
msgid ""
"(3/4) Ok! Now set the date and the time (dd/mm/yyyy HH:mm format, eg "
"30/10/1970 15:33)"
msgstr ""
"(3/4) Ok! Ora imposta la data e l’orario (nel formato gg/mm/aaaa HH:mm, ad "
"esempio 30/10/1980 15:33)"

#: Marvin/modules/events/event_command.py:205
msgid "The date is in the past, insert correct date!"
msgstr "La data che hai impostato è nel passato, inserisci una data corretta!"

#: Marvin/modules/events/event_command.py:214
msgid "(4/4) Last step! Set the location or /skip"
msgstr "(4/4) Ultimo passo, imposta una località oppure /skip"

#: Marvin/modules/events/event_command.py:226
#: Marvin/modules/events/event_command.py:236
msgid "Yeah!!! Event created"
msgstr "Yeah!!! Evento creato con successo"

#: Marvin/modules/events/event_command.py:243
msgid "There is no draft open for user {}"
msgstr "Non c’è nessuna bozza aperta per l’utente corrente {}"

#: Marvin/modules/events/event_inline.py:123
msgid "Users confirmed (<b>{total}</b>)\n"
msgstr "Utenti confermati (<b>{total}</b>)\n"

#: Marvin/modules/events/event_inline.py:133
msgid "Users not confirmed ({total})\n"
msgstr "Utenti non confermati ({total})\n"

#: Marvin/modules/events/event_inline.py:143
msgid "Users maybe ({total})\n"
msgstr "Utenti in forse ({total})\n"

#: Marvin/modules/events/event_inline.py:156
msgid "Join"
msgstr "Ok"

#: Marvin/modules/events/event_inline.py:160
msgid "Not go"
msgstr "Non posso"

#: Marvin/modules/events/event_inline.py:164
msgid "Maybe"
msgstr "In forse"

#: Marvin/modules/events/event_model.py:47
msgid "No user_id found in dictionary"
msgstr "Nessun user_id trovato"

#~ msgid "Create new event..."
#~ msgstr "Crea nuovo evento"
